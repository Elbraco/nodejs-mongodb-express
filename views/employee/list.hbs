<h2>LISTE</h2>
<table class="table table-striped">
    <thead>
        <tr>
        <th>fullName</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>City</th>
        <th></th>

        </tr>

        
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td> {{this.fullName}} </td>
            <td> {{this.Email}} </td>
            <td> {{this.Mobile}} </td>
            <td> {{this.City}} </td>
            <td>
                <a href="/employee/{{this._id}}">Modifier</a>
                <a href="/employee/delete/{{this._id}}">Supprimer</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<a href="/employee">Create New</a>